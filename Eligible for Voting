#include<stdio.h>
#include<conio.h>

int main(){
    int number = 18;
    int age;
    char Name[20];

     printf("Name: ");
    scanf("%[^\n]s", Name);
    printf("age: ");
    scanf("%d", &age);

    printf("name: %s\n", Name);
    printf("age: %d", age);


     if (age >= number){
        printf("\nYou are qualified\n");

     }
     else {
        printf("\nYou are not qualified\n");

     }




    return 0;
}
